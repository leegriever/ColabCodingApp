{
  "name": "has-pnpm",
  "version": "1.1.1",
  "description": "Check if a project is using pnpm.",
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "tslint -p .",
    "pretest": "npm run build",
    "test": "npm run lint && jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/gluons/has-pnpm.git"
  },
  "keywords": [
    "pnpm",
    "has",
    "detect",
    "npm"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/gluons/has-pnpm/issues"
  },
  "homepage": "https://gitlab.com/gluons/has-pnpm",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "figures": "^3.0.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@gluons/prettier-config": "^2.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "8",
    "@types/yargs": "^13.0.0",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "tsconfig-gluons": "^1.0.1",
    "tslint": "^5.16.0",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.4.4"
  }
}
