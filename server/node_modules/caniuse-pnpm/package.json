{
  "name": "caniuse-pnpm",
  "version": "1.1.0",
  "description": "Check if pnpm is available.",
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "tslint -p .",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/gluons/caniuse-pnpm.git"
  },
  "keywords": [
    "pnpm",
    "detect",
    "which",
    "exist"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/gluons/caniuse-pnpm/issues"
  },
  "homepage": "https://gitlab.com/gluons/caniuse-pnpm",
  "dependencies": {
    "chalk": "^2.4.2",
    "figures": "^3.0.0",
    "shelljs": "^0.8.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@gluons/prettier-config": "^2.0.0",
    "@types/node": "8",
    "@types/shelljs": "^0.8.5",
    "@types/yargs": "^13.0.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "tsconfig-gluons": "^1.0.1",
    "tslint": "^5.16.0",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.4.4"
  }
}
